		function callBackForMenu(item){
			var p = item.attr("p");
			if(!!p){
				var a = $('<div></div>')
				.html('hello~')
				.dialog({
					title: item.html()
					,height:"700"
					,width:"600"
					,close: function(event, ui) { $(this).remove(); }
				});
			}
		}
